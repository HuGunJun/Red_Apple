<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iwind.red_apple">


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:name=".App.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 启动页面-->
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 登陆-->
        <activity android:name=".LoginActivity"></activity>

        <!-- 注册-->
        <activity android:name=".RegisterActivity"></activity>

        <!-- 主界面-->
        <activity android:name=".MainActivity"></activity>

        <!-- 税务首页-->
        <activity android:name=".Tax.TaxActivity"></activity>

        <!-- 发现首页-->
        <activity android:name=".Find.FindActivity"></activity>

        <!-- 我的-->
        <activity android:name=".Mine.MineActivity"></activity>

        <!-- 税务常识-->
        <activity android:name=".Tax.TaxKnowActivity"></activity>

        <!-- 税务须知-->
        <activity android:name=".Tax.ShouldKnowActivity"></activity>

        <!-- 添加回答-->
        <activity android:name=".Tax.AddAnswerActivity"></activity>

        <!-- 咨询详情页面-->
        <activity android:name=".Tax.NewsDetailActivity"></activity>

        <!-- 详情页面-->
        <activity android:name=".Tax.DetailActivity"></activity>

        <!--个人税务页面 -->
        <activity android:name=".Tax.PersonalTaxActivity"></activity>

        <!-- 回答详情 -->
        <activity android:name=".Tax.AnswerDetailActivity"></activity>

        <!-- 讨论页面-->
        <activity android:name=".Tax.DiscussActivity"></activity>

        <!-- 新增讨论页面-->
        <activity android:name=".Tax.AddDiscussActivity"></activity>

        <!-- 讨论详情页面-->
        <activity android:name=".Tax.DiscussDetailActivity"></activity>

        <!-- 咨询页面-->
        <activity android:name=".Tax.NewActivity"></activity>

        <!-- 我的信息页面-->
        <activity android:name=".Mine.MyInfoActivity"></activity>

        <!-- 编辑昵称页面-->
        <activity android:name=".Mine.EditNickActivity"></activity>

        <!-- 我的提问-->
        <activity android:name=".Mine.MyQuestionActivity"></activity>

        <!-- 我的回答-->
        <activity android:name=".Mine.MyAnswerActivity"></activity>

        <!-- 我的收藏-->
        <activity android:name=".Mine.MyCollectionActivity"></activity>

        <!-- 用户反馈-->
        <activity android:name=".Mine.UserCallBackActivity"></activity>

        <!-- 设置-->
        <activity android:name=".Mine.SettingActivity"></activity>

        <!-- 修改密码-->
        <activity android:name=".Mine.Act_ChangePass"></activity>

        <!-- 关于我们-->
        <activity android:name=".Mine.Act_AboutUs"></activity>

    </application>

</manifest>
